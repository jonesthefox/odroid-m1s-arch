# Config env for build_u-boot.sh

# Full path to the directory you want to clone the git repos used
ROOTPATH="/usr/src"
# Full path to your custom u-boot .config file
CUSTOM_CONFIG="./u-boot.config"

REPO_UBOOT="https://source.denx.de/u-boot/u-boot.git"
REPO_UBOOT_BRANCH="master"
REPO_RKBIN="https://github.com/rockchip-linux/rkbin.git"

DEFCONFIG="odroid-m1s-rk3566_defconfig"
ARCH="arm64"
VARIANT="RK3566"
BAUDRATE=1500000
# SEC_FIRMWARE_RET=0xa00000
